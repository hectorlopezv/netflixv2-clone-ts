 depending on search\n    const [searchTerm, setsearchTerm] = useState('');//input element\n\n    const firebase = useSelector((stateCurrent: any) => stateCurrent.App.firebase);\n    const user = firebase?.auth()?.currentUser || {};\n    const displayName = profile.displayName;\n\n    useEffect(() => {//navigate to diferrent page\n        //if displayNAME ? homepage\n\n        setTimeout(() => {\n            setLoading(false)\n        }, 3000);\n    }, [displayName]);//execute when depency changes\n\n    useEffect(() => {\n        //active slides (films or series)\n        //fetch series or films\n        setslideRows(slides[category]);//activate either fill or series arrays\n    }, [category, slides]);\n\n    useEffect(() => {\n        //search on arrays the keys that match!\n        const fuse = new Fuse(slideRows, {\n            keys: ['data.description', 'data.title', 'data.genre']\n        })\n        const results = fuse.search(searchTerm).map(({item}) => item);\n\n        //if slides container more...\n        if(slideRows.length > 0 && searchTerm.length > 3 && results.length > 0){\n            setslideRows(results);\n        }\n        else {\n            setslideRows(slides[category]);\n        }\n    }, [searchTerm]);//execute each time effect changes!\n    //if there is a profile clicked \n    return profile.displayName ? (\n        <>\n            {Loading ? < Loading_ src={user.photoURL}/> : <Loading_ReleaseBody />}\n            <Header src=\"joker1\" dontShowSmallVp={true}>\n\n            <Header_Frame>\n                <Header_Group>\n                    <Header_Logo to={ROUTES.HOME} alt=\"Netflix\" src={logo} />\n                    <Header_TextLink active={category === 'series'? 'true' : 'false'}\n                        onClick={() => setcategory('series')}>Series</Header_TextLink>\n                    <Header_TextLink active={category === 'films'? 'true' : 'false'} \n                        onClick={() => setcategory('films')}>Films</Header_TextLink> \n                </Header_Group>\n\n                <Header_Group>\n                    <Header_Search searchTerm={searchTerm} setsearchTerm={setsearchTerm} />\n                    <Header_Profile>\n                        <Header_Picture src={user.photoURL} />\n                        <Header_Dropdown>\n\n                            <Header_Group>\n                                <Header_Picture  src={user.photoURL}/>\n                                <Header_TextLink>{user.displayName}</Header_TextLink>\n                            </Header_Group>\n\n                            <Header_Group>\n                                <Header_TextLink onClick={() => firebase.auth().signOut()}>Sign Out</Header_TextLink>\n                            </Header_Group>\n\n                        </Header_Dropdown>\n\n\n                    </Header_Profile>\n                </Header_Group>\n\n\n            </Header_Frame>\n\n                <Header_Feature>\n                    <Header_FeatureCallOut>\n                        Watch Joker Now\n                    </Header_FeatureCallOut>\n                    <Header_Text>\n                        Forever alone in a crowd, failed comedian Arthur Fleck seeks connection as he walks the streets of Gotham\n                        City. Arthur wears two masks -- the one he paints for his day job as a clown, and the guise he projects in a\n                        futile attempt to feel like he's part of the world around him.\n                    </Header_Text>\n                   \n                    <Header_Group>\n                        <Header_PlayButton><PlayIcon /> Play</Header_PlayButton>\n                    </Header_Group>\n       \n                </Header_Feature>\n            </Header>\n            <Card_Group>\n                {slideRows.map((slideItem: any) => (\n                    <Card key={`${category}-${slideItem.title.toLowerCase()}`}>{/* carc container*/}\n                        <Card_Title>{slideItem.title}</Card_Title>\n                        <Card_Entities>{/*Card Carrousel*/}\n                            {/* Card Item*/}\n                            {slideItem.data.map((item: any) => ( \n                                <Card_Item key={item.docId} item={item}>\n                                    <Card_Image src={`/images/${category}/${item.genre}/${item.slug}/small.jpg`}/>\n                                    <Card_Meta>\n                                        <Card_SubTitle>{item.title}</Card_SubTitle>\n                                        <Card_Text>{item.description}</Card_Text>\n                                    </Card_Meta>\n                                </Card_Item>\n                            ))}\n                        </Card_Entities>\n                        <Card_Feature category={category}>\n                            <Player>\n                                <Player_Button />\n                                <Player_Video src={\"/videos/bunny.mp4\"}/>\n                            </Player>\n                        </Card_Feature>\n\n                    </Card> \n                ))}\n            </Card_Group>\n            <FooterContainer />\n\n        </>\n        ): (\n            <SelectProfileContainer user={user} setProfile={setprofile}/>\n        );\n        \n}\n \nexport default BrowseContainer;","/home/hvmlopez/netflixclone/src/containers/Profiles/Profile.tsx",[],"/home/hvmlopez/netflixclone/src/components/Profiles/index.tsx",[],"/home/hvmlopez/netflixclone/src/components/Profiles/styles/Profilex.tsx",[],"/home/hvmlopez/netflixclone/src/components/Loading/index.tsx",[],"/home/hvmlopez/netflixclone/src/components/Loading/styles/Loading.tsx",[],"/home/hvmlopez/netflixclone/src/components/Card/index.tsx",["246"],"/* eslint-disable react/jsx-pascal-case */\nimport React, { useState, useContext, createContext } from 'react';\nimport { \n    Container,\n    Title, \n    Group ,\n    SubTitle ,\n    Text ,\n    Feature,\n    Feature_Title ,\n    Feature_Text ,\n    Feature_Close ,  \n    maturity as Maturity,\n    Content,\n    Meta,\n    Item ,\n    Image, \n    Entities,\n    } from './styles/Card';\n\n\nexport const FeatureContext = createContext<{[prop:string]:any}>({});\n\nexport interface CardProps {\n\n}\n\nconst Card: React.FC<CardProps> = ({ children, ...props }) => {\n    //when we click card we need a state to show or not Feautures\n    ///we want to show only one feaute in one group\n\n    const [showFeature, setshowFeature] = useState(false);\n    const [itemFeature, setitemFeature] = useState({});\n    return (  \n        <FeatureContext.Provider value={{showFeature, setshowFeature, itemFeature, setitemFeature}} >\n            <Container {...props}>{children}</Container>\n        </FeatureContext.Provider>\n\n    );\n}\nexport default Card;\n\n\nexport interface Card_GroupProps {\n    \n}\n \nexport const Card_Group: React.FC<Card_GroupProps> = ({ children, ...props }) => {\n    return (  \n        <Group {...props}>{children}</Group>\n    );\n}\n \n\nexport interface Card_TitleProps {\n    \n}\n \nexport const Card_Title: React.FC<Card_TitleProps> = ({ children, ...props }) => {\n    return (  <Title {...props}>{children}</Title>);\n}\n \nexport interface Card_SubTitleProps {\n    \n}\n \nexport const Card_SubTitle: React.FC<Card_SubTitleProps> = ({ children, ...props }) => {\n    return ( <SubTitle {...props}>{children}</SubTitle> );\n}\n \nexport interface Card_TextProps {\n    \n}\n \nexport const Card_Text: React.FC<Card_TextProps> = ({ children, ...props }) => {\n    return ( <Text {...props}>{children}</Text> );\n}\n \n\nexport interface Card_MetaProps {\n    \n}\n \nexport const Card_Meta: React.FC<Card_MetaProps> =  ({ children, ...props }) => {\n    //description of the series\n    //holder div for the info\n    return ( <Meta {...props}>{children}</Meta> );\n}\n \nexport interface Card_ItemProps {\n    item: any;\n}\n \nexport const Card_Item: React.FC<Card_ItemProps> =  ({ item, children, ...props }) => {\n    //we we use the shared state\n    const {setshowFeature, setitemFeature} = useContext(FeatureContext);\n    return ( <Item onClick={() => {\n        setitemFeature(item);\n        setshowFeature(true);\n    }}\n        {...props}>{children}</Item> );\n}\n \nexport interface Card_ImageProps {\n    src: string;\n}\n \nexport const Card_Image: React.FC<Card_ImageProps> = ({...props}) => {\n    return ( <Image {...props} /> );\n}\n\n\nexport interface Card_EntitiesProps {\n    \n}\n \nexport const Card_Entities: React.FC<Card_EntitiesProps> = ({children, ...props}) => {\n    return ( <Entities {...props}>{children}</Entities> );\n}\n \n\nexport interface Card_FeatureProps {\n    category: any;\n}\n\n//the modal with react PORTAL\nexport const Card_Feature: React.FC<Card_FeatureProps> = ({category, children, ...props}) => {\n    const {showFeature, setshowFeature, itemFeature, setitemFeature} = useContext(FeatureContext);\n    return showFeature ? (\n        <Feature \n            src={`/images/${category}/${itemFeature.genre}/${itemFeature.slug}/large.jpg`} \n            {...props}\n        >\n            <Content>\n                <Feature_Title>{itemFeature.title}</Feature_Title>\n                <Feature_Text>{itemFeature.description}</Feature_Text>\n                \n                <Feature_Close onClick={() => setshowFeature(false)}>\n                    <img src={\"/images/icons/close.png\"} alt=\"Close\"/>\n                </Feature_Close>\n                {children}\n            </Content>\n\n            <Group margin=\"30px 0\" flexDireciton=\"row\" alignItems=\"center\">\n                <Maturity rating={itemFeature.maturity}>\n                    {itemFeature.maturity < 12 ? 'PG' : itemFeature.maturity}\n                </Maturity>\n                <Feature_Text fontWeight=\"bold\">\n                    {itemFeature.genre.charAt(0).toUpperCase() + itemFeature.genre.slice(1)}\n                </Feature_Text>\n            </Group>\n        </Feature> \n    ): null;\n}\n \n","/home/hvmlopez/netflixclone/src/components/Card/styles/Card.tsx",[],"/home/hvmlopez/netflixclone/src/components/Player/index.tsx",["247"],"import React, {useState, useContext, createContext} from 'react';\n//here is the big MODAL using React Portal\nimport ReactDom from 'react-dom';\nimport {Container, Button, Overlay, Inner, Close} from './styles/Player';\n\nexport interface PlayerProps {\n    \n}\nconst PlayerContext = createContext<{[prop: string]: any}>({});\nconst Player: React.FC<PlayerProps> = ({children, ...props}) => {\n    const [showPlayer, setshowPlayer] = useState(false);\n\n    return (  \n        <PlayerContext.Provider value={{showPlayer, setshowPlayer}}>\n            <Container {...props}>{children}</Container>\n        </PlayerContext.Provider> \n    );\n}\n\nexport default Player;\n\n\nexport interface Player_VideoProps {\n    src: string;\n}\n \nexport const Player_Video: React.FC<Player_VideoProps> = ({src, ...props}) => {\n    const { showPlayer, setshowPlayer } = useContext(PlayerContext);\n    return showPlayer ? ReactDom.createPortal(\n    <Overlay data-testid=\"player\" onClick={() => setshowPlayer(false)  }>\n        <Inner>\n            <video id=\"netflix-player\" controls>\n                <source src={src} type=\"video/mp4\" />\n            </video>\n            <Close />\n        </Inner>\n    </Overlay>, document.body): null;\n}\n\n\nexport interface Player_ButtonProps {\n    \n}\n \nexport const Player_Button: React.FC<Player_ButtonProps> = () => {\n    const { showPlayer, setshowPlayer } = useContext(PlayerContext);\n\n    return <Button onClick={() => setshowPlayer( (prevstate: boolean) => !prevstate )}>\n        Play\n    </Button>\n}\n \n\n ","/home/hvmlopez/netflixclone/src/components/Player/styles/Player.tsx",[],{"ruleId":"248","replacedBy":"249"},{"ruleId":"250","replacedBy":"251"},{"ruleId":"248","replacedBy":"252"},{"ruleId":"250","replacedBy":"253"},{"ruleId":"254","severit